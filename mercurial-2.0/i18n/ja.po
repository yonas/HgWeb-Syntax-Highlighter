#, python-format
msgid ""
"[hooks]\n"
"pretxncommit.%s = python:hgext.win32text.forbid%s"
msgstr ""
"[hooks]\n"
"pretxncommit.%s = python:hgext.win32text.forbid%s"

#, python-format
